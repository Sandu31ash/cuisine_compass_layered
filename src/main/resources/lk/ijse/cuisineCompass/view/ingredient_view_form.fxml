        <columns>
          <TableColumn fx:id="colCode" editable="false" prefWidth="213.0" />
          <TableColumn fx:id="colDes" editable="false" prefWidth="238.0" />
            <TableColumn fx:id="colUnit" editable="false" prefWidth="228.0" />
            <TableColumn fx:id="colQty" editable="false" prefWidth="163.0" />
